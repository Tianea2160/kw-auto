{
  "name": "KW Auto",
  "version": "1.1.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "./popup/popup.html",
    "default_icon": {
      "8": "/asset/icon/KWAuto.png",
      "16": "/asset/icon/icon_default_1.png",
      "32": "/asset/icon/icon_default_2.png",
      "48": "/asset/icon/icon_default_3.png",
      "128": "/asset/icon/icon_default_4.png"
    }
  },
  "icons": {
    "8": "/asset/icon/KWAuto.png",
    "16": "/asset/icon/icon_default_1.png",
    "32": "/asset/icon/icon_default_2.png",
    "48": "/asset/icon/icon_default_3.png",
    "128": "/asset/icon/icon_default_4.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.kw.ac.kr/*"
      ],
      "js": [
        "main.js"
      ]
    },
    {
      "matches": [
        "https://klas.kw.ac.kr/usr/cmn/login/LoginForm.do*"
      ],
      "js": [
        "login/login.js"
      ]
    },
    {
      "matches": [
        "https://klas.kw.ac.kr/std/cmn/frame/Frame.do*"
      ],
      "css": [
        "homework/homework.css",
        "folding/folding.css"
      ],
      "js": [
        "homework/homework.js",
        "folding/folding.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://klas.kw.ac.kr/std/cps/inqire/IndividualPortfolio.do"
      ],
      "js" : [
        "portfolio/portfolio.js"
      ],
      "css" : [
        "portfolio/portfolio.css"
      ],
      "run_at": "document_end"
    }
  ]
}